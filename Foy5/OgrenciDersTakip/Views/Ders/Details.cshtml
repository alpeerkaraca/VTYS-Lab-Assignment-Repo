@model OgrenciDersTakip.Models.TDers
@{
    ViewData["Title"] = "Ders Detay";
}
<h2>Ders Detay</h2>
<div>
    <dl class="row">
        <dt class="col-sm-2">Ders Adı</dt>
        <dd class="col-sm-10">@Model.DersAd</dd>
    </dl>
    <a asp-action="Edit" asp-route-id="@Model.DersID" class="btn btn-primary">Düzenle</a>
    <a asp-action="Index" class="btn btn-secondary">Geri</a>
</div>